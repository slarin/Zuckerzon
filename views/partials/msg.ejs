<% if(typeof errors != 'undefined') { %>
  <% errors.forEach(function(error) { %>
    <p class="msg-txt"><%= error.msg %></p>
  <% }); %>
<% } %>

<% if(success_msg != '') { %>
  <p class="msg-txt"><%= success_msg %></p>
<% } %>

<% if(error_msg != '') { %>
  <p class="msg-txt"><%= error_msg %></p>
<% } %>

<% if(error != '') { %>
  <p class="msg-txt"><%= error_msg %></p>
<% } %>